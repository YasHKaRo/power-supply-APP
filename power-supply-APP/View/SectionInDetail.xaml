<UserControl x:Class="power_supply_APP.View.SectionInDetail"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="20*"></ColumnDefinition>
                <ColumnDefinition Width="215*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid Grid.Column="0">
                <Grid.RowDefinitions>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                    <RowDefinition></RowDefinition>
                </Grid.RowDefinitions>
                <Ellipse Grid.Row="0" Name="RedIndicatorMainCell" Fill="DarkRed" Width="40" Height="40" Margin="0" />
                <Ellipse Stroke="Black" StrokeThickness="1" Width="10" Height="10" Margin="15" />
                <Ellipse Stroke="Black" StrokeThickness="1" Width="20" Height="20" Margin="10" />
                <Ellipse Stroke="Black" StrokeThickness="1" Width="30" Height="30" Margin="5" />
                <Ellipse Stroke="Black" StrokeThickness="1" Width="40" Height="40" Margin="0" />
                <Ellipse Grid.Row="1" Name="GreenIndicatorMainCell" Fill="DarkGreen" Width="40" Height="40" Margin="0" />
                <Ellipse Grid.Row="1" Stroke="Black" StrokeThickness="1" Width="10" Height="10" Margin="15" />
                <Ellipse Grid.Row="1" Stroke="Black" StrokeThickness="1" Width="20" Height="20" Margin="10" />
                <Ellipse Grid.Row="1" Stroke="Black" StrokeThickness="1" Width="30" Height="30" Margin="5" />
                <Ellipse Grid.Row="1" Stroke="Black" StrokeThickness="1" Width="40" Height="40" Margin="0" />
                <Canvas Grid.Row="3" Margin="10">

                    <!-- Черная область между прямоугольником и окружностью -->
                    <Rectangle Width="40" Height="35" Fill="Black" Canvas.Left="5" Canvas.Top="0"/>

                    <!-- Прямоугольник с текстом "СТОП" -->
                    <Rectangle Width="36" Height="12" Fill="White" Canvas.Left="7" Canvas.Top="2"/>
                    <TextBlock Text="СТОП" Foreground="Black" FontSize="12" Canvas.Left="10" />
                    <!-- Окружность -->
                    <Ellipse Width="40" Height="40" Fill="Transparent" Stroke="Black" Canvas.Left="5" Canvas.Top="15"/>
                    <!-- Кнопка внутри окружности -->
                    <Button Content="" Width="40" Height="40" Canvas.Left="5" Canvas.Top="15" Background="Red" Click="Stop_Click">
                        <Button.Resources>
                            <Style TargetType="Border">
                                <Setter Property="CornerRadius" Value="50"/>
                            </Style>
                        </Button.Resources>
                    </Button>

                </Canvas>
                <Canvas Grid.Row="4" Margin="10">

                    <!-- Черная область между прямоугольником и окружностью -->
                    <Rectangle Width="40" Height="35" Fill="Black" Canvas.Left="5" Canvas.Top="0"/>

                    <!-- Прямоугольник с текстом "СТОП" -->
                    <Rectangle Width="36" Height="12" Fill="White" Canvas.Left="7" Canvas.Top="2"/>
                    <TextBlock Text="ПУСК" Foreground="Black" FontSize="12" Canvas.Left="10" />
                    <!-- Окружность -->
                    <Ellipse Width="40" Height="40" Fill="Transparent" Stroke="Black" Canvas.Left="5" Canvas.Top="15"/>
                    <!-- Кнопка внутри окружности -->
                    <Button Content="" Width="40" Height="40" Canvas.Left="5" Canvas.Top="15" Background="LightGreen" Click="Start_Click">
                        <Button.Resources>
                            <Style TargetType="Border">
                                <Setter Property="CornerRadius" Value="50"/>
                            </Style>
                        </Button.Resources>
                    </Button>

                </Canvas>
            </Grid>
            <Grid Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="10*"></RowDefinition>
                    <RowDefinition Height="20*"></RowDefinition>
                    <RowDefinition Height="80*"></RowDefinition>
                    <RowDefinition Height="30*"></RowDefinition>
                    <RowDefinition Height="3*"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="40*"></ColumnDefinition>
                        <ColumnDefinition Width="60*"></ColumnDefinition>
                        <ColumnDefinition Width="30*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Button Margin="15" Height="30" ToolTip="Добавить БП" x:Name="AddPowerUnit" Click="AddPowerUnit_Click" Tag="{Binding SectionIndex}">Добавить</Button>
                    <Grid Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <TextBlock FontSize="16" TextWrapping="Wrap" >Прибор:</TextBlock>
                        <TextBlock Grid.Row="1" FontSize="16" TextWrapping="Wrap">Заводской номер:</TextBlock>
                        <TextBlock FontSize="16" Grid.Column="1" TextWrapping="Wrap" x:Name="DetailName"></TextBlock>
                        <TextBlock Grid.Row="1" Grid.Column="1" FontSize="16" TextWrapping="Wrap" x:Name="DetailNumb"></TextBlock>

                    </Grid>
                    <TextBlock Grid.Column="2" FontSize="16" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center">Индикаторы</TextBlock>
                </Grid>
                <Grid Grid.Row="2" x:Name="MainWindowMainCell">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20*"></RowDefinition>
                        <RowDefinition Height="62*"></RowDefinition>
                        <RowDefinition Height="62*"></RowDefinition>
                        <RowDefinition Height="62*"></RowDefinition>
                        <RowDefinition Height="62*"></RowDefinition>
                        <RowDefinition Height="62*"></RowDefinition>
                        <RowDefinition Height="62*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="100*"></ColumnDefinition>
                        <ColumnDefinition Width="200*"></ColumnDefinition>
                        <ColumnDefinition Width="120*"></ColumnDefinition>
                        <ColumnDefinition Width="120*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Row="1" FontSize="12" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,10,0,0">Энергоциклирование</TextBlock>
                    <TextBlock Grid.Row="2" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Top"  Margin="0,10,0,0">Iхх</TextBlock>
                    <TextBlock Grid.Row="3" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Top"  Margin="0,10,0,0">Iзащ</TextBlock>
                    <TextBlock Grid.Row="4" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Top"  Margin="0,10,0,0">Iкз</TextBlock>
                    <TextBlock Grid.Row="5" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Top"  Margin="0,10,0,0">Uпульс</TextBlock>

                    <Border BorderBrush="Black" Grid.Row="1" Grid.ColumnSpan="4" Grid.RowSpan="5" BorderThickness="1"></Border>
                    <Border BorderBrush="Black" Grid.Row="2" Grid.ColumnSpan="4" Grid.RowSpan="4" BorderThickness="1"></Border>
                    <Border BorderBrush="Black" Grid.Row="3" Grid.ColumnSpan="4" Grid.RowSpan="3" BorderThickness="1"></Border>
                    <Border BorderBrush="Black" Grid.Row="4" Grid.ColumnSpan="4" Grid.RowSpan="2" BorderThickness="1"></Border>
                    <Border BorderBrush="Black" Grid.Row="5" Grid.ColumnSpan="4" Grid.RowSpan="1" BorderThickness="1"></Border>
                    <TextBlock Grid.Row="6" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center">Таймер</TextBlock>
                    <TextBlock Grid.Row="6" Grid.Column="1" x:Name="timeTextBlock" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" Margin="0">
                      00:00:00
                    </TextBlock>

                    <TabControl x:Name="Charts" Grid.Row="0" Grid.RowSpan="6" Grid.Column="1" Grid.ColumnSpan="2" Margin="0,-10,0,0">
                        <TabItem x:Name="CurrentCharts">
                            <TabItem.Header>
                                <TextBlock>Сила тока</TextBlock>
                            </TabItem.Header>
                            <TabItem.Content>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="120*"></ColumnDefinition>
                                        <ColumnDefinition Width="40*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <lvc:CartesianChart Grid.Row="0" Name="Current_Chart_En"></lvc:CartesianChart>
                                    <lvc:CartesianChart Grid.Row="1" Name="Current_Chart_Ihh"></lvc:CartesianChart>
                                    <lvc:CartesianChart Grid.Row="2" Name="Current_Chart_Iprot"></lvc:CartesianChart>
                                    <lvc:CartesianChart Grid.Row="3" Name="Current_Chart_Ikz"></lvc:CartesianChart>
                                    <lvc:CartesianChart Grid.Row="4" Name="Current_Chart_Upuls"></lvc:CartesianChart>
                                    <Grid Grid.Column="1" Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Column="0" Grid.Row="0" Margin="5"></TextBox>
                                        <TextBox Grid.Column="0" Grid.Row="1" Margin="5"></TextBox>
                                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5,0" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Avg. А</TextBlock>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5,0" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Max. А</TextBlock>
                                    </Grid>
                                    <Grid Grid.Column="1" Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Column="0" Grid.Row="0" Margin="5"></TextBox>
                                        <TextBox Grid.Column="0" Grid.Row="1" Margin="5"></TextBox>
                                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5,0" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Avg. А</TextBlock>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5,0" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Max. А</TextBlock>
                                    </Grid>
                                    <Grid Grid.Column="1" Grid.Row="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Column="0" Grid.Row="0" Margin="5"></TextBox>
                                        <TextBox Grid.Column="0" Grid.Row="1" Margin="5"></TextBox>
                                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5,0" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Avg. А</TextBlock>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5,0" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Max. А</TextBlock>
                                    </Grid>
                                    <Grid Grid.Column="1" Grid.Row="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Column="0" Grid.Row="0" Margin="5"></TextBox>
                                        <TextBox Grid.Column="0" Grid.Row="1" Margin="5"></TextBox>
                                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5,0" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Avg. А</TextBlock>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5,0" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Max. А</TextBlock>
                                    </Grid>
                                    <Grid Grid.Column="1" Grid.Row="4">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Column="0" Grid.Row="0" Margin="5"></TextBox>
                                        <TextBox Grid.Column="0" Grid.Row="1" Margin="5"></TextBox>
                                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5,0" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Avg. А</TextBlock>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5,0" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Max. А</TextBlock>
                                    </Grid>
                                </Grid>

                            </TabItem.Content>
                        </TabItem>
                        <TabItem x:Name="VoltageCharts">
                            <TabItem.Header>
                                <TextBlock>Напряжение</TextBlock>
                            </TabItem.Header>
                            <TabItem.Content>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="120*"></ColumnDefinition>
                                        <ColumnDefinition Width="40*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <lvc:CartesianChart Grid.Row="0" Name="Voltage_Chart_En"></lvc:CartesianChart>
                                    <lvc:CartesianChart Grid.Row="1" Name="Voltage_Chart_Ihh"></lvc:CartesianChart>
                                    <lvc:CartesianChart Grid.Row="2" Name="Voltage_Chart_Iprot"></lvc:CartesianChart>
                                    <lvc:CartesianChart Grid.Row="3" Name="Voltage_Chart_Ikz"></lvc:CartesianChart>
                                    <lvc:CartesianChart Grid.Row="4" Name="Voltage_Chart_Upuls"></lvc:CartesianChart>
                                    <Grid Grid.Column="1" Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Column="0" Grid.Row="0" Margin="5"></TextBox>
                                        <TextBox Grid.Column="0" Grid.Row="1" Margin="5"></TextBox>
                                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Avg. А</TextBlock>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Max. А</TextBlock>
                                    </Grid>
                                    <Grid Grid.Column="1" Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Column="0" Grid.Row="0" Margin="5"></TextBox>
                                        <TextBox Grid.Column="0" Grid.Row="1" Margin="5"></TextBox>
                                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Avg. В</TextBlock>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Max. В</TextBlock>
                                    </Grid>
                                    <Grid Grid.Column="1" Grid.Row="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Column="0" Grid.Row="0" Margin="5"></TextBox>
                                        <TextBox Grid.Column="0" Grid.Row="1" Margin="5"></TextBox>
                                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Avg. В</TextBlock>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Max. В</TextBlock>
                                    </Grid>
                                    <Grid Grid.Column="1" Grid.Row="3">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Column="0" Grid.Row="0" Margin="5"></TextBox>
                                        <TextBox Grid.Column="0" Grid.Row="1" Margin="5"></TextBox>
                                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Avg. В</TextBlock>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Max. В</TextBlock>
                                    </Grid>
                                    <Grid Grid.Column="1" Grid.Row="4">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <TextBox Grid.Column="0" Grid.Row="0" Margin="5"></TextBox>
                                        <TextBox Grid.Column="0" Grid.Row="1" Margin="5"></TextBox>
                                        <TextBlock Grid.Column="1" Grid.Row="0" Margin="5" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Avg. В</TextBlock>
                                        <TextBlock Grid.Column="1" Grid.Row="1" Margin="5" FontSize="10" HorizontalAlignment="Center" VerticalAlignment="Center">Max. В</TextBlock>
                                    </Grid>
                                </Grid>
                            </TabItem.Content>
                        </TabItem>
                    </TabControl>
                    <Grid Grid.Column="3" Grid.Row="1">
                        <Ellipse Grid.Row="0" Fill="{Binding IndicatorEnergy, RelativeSource={RelativeSource AncestorType=UserControl}}" Width="40" Height="40" Margin="0" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="10" Height="10" Margin="15" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="20" Height="20" Margin="10" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="30" Height="30" Margin="5" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="40" Height="40" Margin="0" />
                    </Grid>
                    <Grid Grid.Column="3" Grid.Row="2">
                        <Ellipse Grid.Row="0" Fill="{Binding IndicatorIhh, RelativeSource={RelativeSource AncestorType=UserControl}}" Width="40" Height="40" Margin="0" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="10" Height="10" Margin="15" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="20" Height="20" Margin="10" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="30" Height="30" Margin="5" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="40" Height="40" Margin="0" />
                    </Grid>
                    <Grid Grid.Column="3" Grid.Row="3">
                        <Ellipse Grid.Row="0" Fill="{Binding IndicatorIprot, RelativeSource={RelativeSource AncestorType=UserControl}}" Width="40" Height="40" Margin="0" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="10" Height="10" Margin="15" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="20" Height="20" Margin="10" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="30" Height="30" Margin="5" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="40" Height="40" Margin="0" />
                    </Grid>
                    <Grid Grid.Column="3" Grid.Row="4">
                        <Ellipse Grid.Row="0" Fill="{Binding IndicatorIkz, RelativeSource={RelativeSource AncestorType=UserControl}}" Width="40" Height="40" Margin="0" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="10" Height="10" Margin="15" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="20" Height="20" Margin="10" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="30" Height="30" Margin="5" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="40" Height="40" Margin="0" />
                    </Grid>
                    <Grid Grid.Column="3" Grid.Row="5">
                        <Ellipse Grid.Row="0" Fill="{Binding IndicatorUpuls, RelativeSource={RelativeSource AncestorType=UserControl}}" Width="40" Height="40" Margin="0" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="10" Height="10" Margin="15" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="20" Height="20" Margin="10" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="30" Height="30" Margin="5" />
                        <Ellipse Stroke="Black" StrokeThickness="1" Width="40" Height="40" Margin="0" />
                    </Grid>
                </Grid>

            </Grid>
        </Grid>
    </Grid>
</UserControl>
