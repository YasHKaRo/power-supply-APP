<Page x:Class="power_supply_APP.SettingsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:power_supply_APP"
      mc:Ignorable="d" 
      d:DesignHeight="800" d:DesignWidth="1300"
      Title="SettingsPage">

    <StackPanel>
        <Button Content="Х" Click="BackButton_Click" Width="20" Margin="10" Background="Red" HorizontalAlignment="Right"/>
        <TabControl x:Name="settings" Height="700">
            <TabItem x:Name="generalSettingsOfCells">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Ellipse Height="10" Width="10" Fill="Black" />
                        <TextBlock Margin="3">Общие параметры стенда</TextBlock>
                    </StackPanel>
                </TabItem.Header>
                <TabItem.Content>
                    <TabControl TabStripPlacement="Right" x:Name="generalSettings">
                        <TabControl.Resources>
                            <Style TargetType="TabItem">
                                <Setter Property="Width" Value="100"/>
                                <!-- Установка ширины вкладок -->
                                <Setter Property="Height" Value="60"/>
                                <!-- Установка высоты вкладок -->
                            </Style>
                        </TabControl.Resources>
                        <TabItem x:Name="modes">
                            <TabItem.Header>
                                <StackPanel Orientation="Horizontal">
                                    <Ellipse Height="10" Width="10" Fill="Black" />
                                    <TextBlock Margin="3" TextWrapping="Wrap">Режимы</TextBlock>
                                </StackPanel>
                            </TabItem.Header>
                            <TabItem.Content>
                                <StackPanel>
                                    <!--Настройка режима Энергоциклирования-->
                                    <GroupBox>
                                        <GroupBox.Header>
                                            <TextBlock FontSize="16">Режим "Испытание"</TextBlock>
                                        </GroupBox.Header>
                                        <StackPanel Orientation="Horizontal">
                                            <WrapPanel Width="1176" Height="400">
                                                <GroupBox Margin="10, 0, 0, 0" Height="390">
                                                    <GroupBox.Header>
                                                        <CheckBox Content="'Энергоциклирования'" FontSize="14" />
                                                    </GroupBox.Header>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="120" />
                                                            <ColumnDefinition Width="40" />
                                                            <ColumnDefinition Width="20" />
                                                            <ColumnDefinition Width="20" />
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <!--Поле ввода информации для min напряжении элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность цикла при min напряжении:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxMin" Grid.Column="0" Grid.Row="1"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="1" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Минут</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxMin" Content="-" Grid.Column="2" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxMin" Content="+" Grid.Column="3" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>

                                                        <!--Поле ввода информации для max напряжении элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность цикла при max напряжении:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxMax" Grid.Column="0" Grid.Row="3"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="3" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Минут</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxMax" Content="-" Grid.Column="2" Grid.Row="3" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0"  VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxMax" Content="+" Grid.Column="3" Grid.Row="3" Click="OnChangeButtonClick" Height="20" Width="20"  VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold">

                                                        </Button>

                                                        <!--Поле ввода информации для одинарного включения элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность для одинарного включения:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxSingle" Grid.Column="0" Grid.Row="5"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="5" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Минут</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxSingle" Content="-"  VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Grid.Column="2" Grid.Row="5" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxSingle" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="5" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>

                                                        <!--Поле ввода информации для паузы между включениями элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="6" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность паузы между включениями:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxPause" Grid.Column="0" Grid.Row="7"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="7" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Секунд</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxPause" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="7" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxPause" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="7" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>
                                                        <CheckBox Grid.Row="8" Grid.ColumnSpan="4">Контроль температуры</CheckBox>
                                                    </Grid>
                                                </GroupBox>
                                                <!--Настройка режима Iхх-->
                                                <GroupBox Margin="20, 0, 0, 0">
                                                    <GroupBox.Header>
                                                        <CheckBox Content="'Измерение Iхх'" FontSize="14" />
                                                    </GroupBox.Header>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="120" />
                                                            <ColumnDefinition Width="40" />
                                                            <ColumnDefinition Width="20" />
                                                            <ColumnDefinition Width="20" />
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <!--Поле ввода информации для одинарного включения элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность одинарного включения:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxSingleHH" Grid.Column="0" Grid.Row="1"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="1" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Минут</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxSingleHH" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxSingleHH" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>

                                                        <!--Поле ввода информации для паузы между включениями элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность паузы между включениями:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxPauseHH" Grid.Column="0" Grid.Row="3"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="3" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Секунд</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxPauseHH" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="3" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxPauseHH" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="3" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>

                                                        <!--Поле ввода информации для воздействия режима ХХ элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность воздействия режима ХХ:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxImpactsHH" Grid.Column="0" Grid.Row="5"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="5" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Секунд</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxImpactsHH" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="5" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxImpactsHH" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="5" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>

                                                        <CheckBox Grid.Row="8" Grid.ColumnSpan="4">Контроль температуры</CheckBox>
                                                    </Grid>
                                                </GroupBox>
                                                <!--Настройка режима Iзащ-->
                                                <GroupBox Margin="20, 0, 0, 0">
                                                    <GroupBox.Header>
                                                        <CheckBox Content="'Измерение Iзащ'" FontSize="14" />
                                                    </GroupBox.Header>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="120" />
                                                            <ColumnDefinition Width="40" />
                                                            <ColumnDefinition Width="20" />
                                                            <ColumnDefinition Width="20" />
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <!--Поле ввода информации для одинарного включения элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность одинарного включения:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxSingleProtect" Grid.Column="0" Grid.Row="1"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="1" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Минут</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxSingleProtect" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxSingleProtect" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>

                                                        <!--Поле ввода информации для паузы между включениями элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность паузы между включениями:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxPauseProtect" Grid.Column="0" Grid.Row="3"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="3" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Секунд</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxPauseProtect" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="3" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxPauseProtect" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="3" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>

                                                        <!--Поле ввода информации для воздействия режима ПОРОГ элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность воздействия режима ПОРОГ:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxImpactsStep" Grid.Column="0" Grid.Row="5"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="5" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Секунд</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxImpactsStep" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="5" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxImpactsStep" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="5" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>

                                                        <CheckBox Grid.Row="8" Grid.ColumnSpan="4">Контроль температуры</CheckBox>
                                                    </Grid>
                                                </GroupBox>
                                                <!--Настройка режима Iкз-->
                                                <GroupBox Margin="20, 0, 0, 0">
                                                    <GroupBox.Header>
                                                        <CheckBox Content="'Измерение Iкз'" FontSize="14" />
                                                    </GroupBox.Header>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="120" />
                                                            <ColumnDefinition Width="40" />
                                                            <ColumnDefinition Width="20" />
                                                            <ColumnDefinition Width="20" />
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <!--Поле ввода информации для одинарного включения элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность одинарного включения:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxSingleKZ" Grid.Column="0" Grid.Row="1"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="1" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Минут</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxSingleKZ" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxSingleKZ" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>

                                                        <!--Поле ввода информации для паузы между включениями элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность паузы между включениями:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxPauseKZ" Grid.Column="0" Grid.Row="3"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="3" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Секунд</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxPauseKZ" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="3" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxPauseKZ" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="3" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>

                                                        <!--Поле ввода информации для воздействия режима КЗ элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность воздействия режима КЗ:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxImpactsKZ" Grid.Column="0" Grid.Row="5"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="5" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Секунд</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxImpactsKZ" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="5" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

                                                        </Button>
                                                        <!-- Кнопка увеличения значения -->
                                                        <Button Tag="TextBoxImpactsKZ" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="+" Grid.Column="3" Grid.Row="5" Click="OnChangeButtonClick" Height="20" Width="20" >

                                                        </Button>

                                                        <CheckBox Grid.Row="8" Grid.ColumnSpan="4">Контроль температуры</CheckBox>
                                                    </Grid>
                                                </GroupBox>
                                                <!--Настройка режима Uпульс-->
                                                <GroupBox Margin="20, 0, 0, 0">
                                                    <GroupBox.Header>
                                                        <CheckBox Content="'Измерение Uпульс'" FontSize="14" />
                                                    </GroupBox.Header>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="120" />
                                                            <ColumnDefinition Width="40" />
                                                            <ColumnDefinition Width="20" />
                                                            <ColumnDefinition Width="20" />
                                                        </Grid.ColumnDefinitions>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <!--Поле ввода информации для одинарного включения элемента-->
                                                        <TextBlock Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="4" TextWrapping="Wrap">Продолжительность одинарного включения:</TextBlock>
                                                        <!-- Поле ввода для значения -->
                                                        <TextBox x:Name="TextBoxSingleVolt" Grid.Column="0" Grid.Row="1"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,0,5,0" />
                                                        <!-- Поле СИ-->
                                                        <TextBlock Grid.Column="1" Grid.Row="1" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">Минут</TextBlock>
                                                        <!-- Кнопка уменьшения значения -->
                                                        <Button Tag="TextBoxSingleVolt" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" Content="-" Grid.Column="2" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0">

