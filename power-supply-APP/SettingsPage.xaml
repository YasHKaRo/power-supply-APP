                                            <TextBlock FontSize="16">Дополнительные режимы</TextBlock>
                                        </GroupBox.Header>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition></RowDefinition>

                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="200*"></ColumnDefinition>
                                                <ColumnDefinition Width="91*"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
