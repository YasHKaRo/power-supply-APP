                                </StackPanel>
                            </TabItem.Header>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <GroupBox Grid.Column="0" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="201" Width="386">
                                    <GroupBox.Header>
                                        <TextBlock >Сеть</TextBlock>
                                    </GroupBox.Header>
                                </GroupBox>
                                <Grid Grid.Row="0" Grid.Column="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Row="0" Grid.Column="0" Margin="15, 15, 0 0" FontSize="14">Минимальное напряжение</TextBlock>
                                    <Grid Grid.Row="0" Grid.Column="1">
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>

                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="70*"/>
                                            <ColumnDefinition Width="29*"/>
                                            <ColumnDefinition Width="70*"/>
                                            <ColumnDefinition Width="29*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBox x:Name="TextBoxMinVolt" Grid.Column="0"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,15,20,0" />
                                        <Grid Grid.Row="0" Grid.Column="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition/>

                                                <RowDefinition/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>

                                            </Grid.ColumnDefinitions>
                                            <Button Tag="TextBoxMinVolt" Content="+" Grid.Column="3" Grid.Row="0" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,15,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>
                                            <Button Tag="TextBoxMinVolt" Content="-" Grid.Column="3" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>
                                            <!-- Конец настроек кнопок напряжения-->
                                        </Grid>
                                        <TextBox x:Name="TextBoxMinVoltApprox" Grid.Column="2"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,15,20,0" />
                                        <Grid Grid.Row="0" Grid.Column="2">
                                            <Grid.RowDefinitions>
                                                <RowDefinition/>

                                                <RowDefinition/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>

                                            </Grid.ColumnDefinitions>
                                            <Button Tag="TextBoxMinVoltApprox" Content="+" Grid.Column="3" Grid.Row="0" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,15,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>
                                            <Button Tag="TextBoxMinVoltApprox" Content="-" Grid.Column="3" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>
                                            <!-- Конец настроек кнопок напряжения-->
                                        </Grid>
                                        <TextBlock Grid.Column="1" Grid.Row="0" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">В +/-</TextBlock>
                                        <TextBlock Grid.Column="3" Grid.Row="0" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Left" VerticalAlignment="Center" FontWeight="Bold" Margin="0,0,10,0">%</TextBlock>

                                    </Grid>
                                    <TextBlock Grid.Row="1" Grid.Column="0" Margin="15, 15, 0 0" FontSize="14">Максимальное напряжение</TextBlock>
                                    <Grid Grid.Row="1" Grid.Column="1">
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>

                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="70*"/>
                                            <ColumnDefinition Width="29*"/>
                                            <ColumnDefinition Width="70*"/>
                                            <ColumnDefinition Width="29*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBox x:Name="TextBoxMaxVolt" Grid.Column="0"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,15,20,0" />
                                        <Grid Grid.Row="0" Grid.Column="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition/>

                                                <RowDefinition/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>

                                            </Grid.ColumnDefinitions>
                                            <Button Tag="TextBoxMaxVolt" Content="+" Grid.Column="2" Click="OnChangeButtonClick" Height="20" Width="20" VerticalAlignment="Center" HorizontalAlignment="Right" FontWeight="Bold" Grid.ColumnSpan="2" Margin="0,15,0,0"/>
                                            <Button Tag="TextBoxMaxVolt" Content="-" Grid.Column="3" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>
                                            <!-- Конец настроек кнопок напряжения-->
                                        </Grid>
                                        <TextBox x:Name="TextBoxMaxVoltApprox" Grid.Column="2"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="0,15,20,0" />
                                        <Grid Grid.Row="0" Grid.Column="2">
                                            <Grid.RowDefinitions>
                                                <RowDefinition/>

                                                <RowDefinition/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>
                                                <ColumnDefinition></ColumnDefinition>

                                            </Grid.ColumnDefinitions>
                                            <Button Tag="TextBoxMaxVoltApprox" Content="+" Grid.Column="3" Grid.Row="0" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,15,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>
                                            <Button Tag="TextBoxMaxVoltApprox" Content="-" Grid.Column="3" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="20" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>
                                            <!-- Конец настроек кнопок напряжения-->
                                        </Grid>
                                        <TextBlock Grid.Column="1" Grid.Row="0" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">В +/-</TextBlock>
                                        <TextBlock Grid.Column="3" Grid.Row="0" TextWrapping="Wrap" FontSize="9" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold">%</TextBlock>

                                    </Grid>
                                    <!-- Поле ввода для значения -->
                                    <!-- Поле СИ-->

                                </Grid>

                                <Grid Grid.Row="1" Grid.Column="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <GroupBox Grid.Column="0" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="201" Width="386" Margin="0, -5">
                                        <GroupBox.Header>
                                            <TextBlock >ПИД-Регулятор</TextBlock>
                                        </GroupBox.Header>
                                        <CheckBox FontSize="14">Режим "ПИД-регулирование"</CheckBox>
                                    </GroupBox>
                                </Grid>
                                <GroupBox Grid.Row="0" Grid.Column="1" Grid.RowSpan="2">Измеритель пульсаций</GroupBox>
                                <Grid Grid.Row="0" Grid.Column="1" Grid.RowSpan="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="173*"></ColumnDefinition>
                                        <ColumnDefinition Width="25*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="40*"></RowDefinition>
                                        <RowDefinition Height="40*"></RowDefinition>
                                        <RowDefinition Height="40*"></RowDefinition>
                                        <RowDefinition Height="40*"></RowDefinition>
                                        <RowDefinition Height="40*"></RowDefinition>
                                        <RowDefinition Height="40*"></RowDefinition>
                                        <RowDefinition Height="40*"></RowDefinition>
                                        <RowDefinition Height="40*"></RowDefinition>
                                        <RowDefinition Height="40*"></RowDefinition>
                                        <RowDefinition Height="80*"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <TextBlock Margin="10, 20, 0 0" FontSize="14">Диапозон измерений</TextBlock>
                                    <ComboBox Grid.Column="0" Margin="0,38,0,38" Grid.RowSpan="3">
                                        <TextBlock>АВТО</TextBlock>
                                        <TextBlock>300мВ</TextBlock>
                                        <TextBlock>3000мВ</TextBlock>
                                    </ComboBox>
                                    <TextBlock Grid.Row="2" Grid.Column="0" Margin="10,20,0,38" FontSize="14" Grid.RowSpan="2">Полоса частот</TextBlock>
                                    <ComboBox Grid.Row="3" Grid.Column="0">
                                        <TextBlock>100МГц</TextBlock>
                                        <TextBlock>20МГц</TextBlock>

                                    </ComboBox>
                                    <TextBlock Grid.Row="4" Grid.Column="0" Margin="10,20,0,38" FontSize="14" Grid.RowSpan="2">Фильтр</TextBlock>
                                    <ComboBox Grid.Row="5" Grid.Column="0" Margin="0,0,0,38" Grid.RowSpan="2">
                                        <TextBlock>Без фильтра</TextBlock>
                                        <TextBlock>HF</TextBlock>
                                        <TextBlock>LF</TextBlock>
                                        <TextBlock>LF+HF</TextBlock>
                                    </ComboBox>
                                    <TextBlock Grid.Row="8" Margin="10,10,0,76" FontSize="14" Grid.RowSpan="2">Поправочный коэффициент</TextBlock>
                                    <TextBox x:Name="CorrectionFactor" Grid.Row="8"  Text="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="200,5,0,5" />
                                    <Grid Grid.Row="7" Grid.Column="1" Margin="0,38,0,76" Grid.RowSpan="3">
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Button Tag="CorrectionFactor" Content="+0.001" Grid.Column="0" Grid.Row="0" Click="OnChangeButtonClick" Height="20" Width="44" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>
                                        <Button Tag="CorrectionFactor" Content="-0.001" Grid.Column="0" Grid.Row="1" Click="OnChangeButtonClick" Height="20" Width="44" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold"/>
                                        <!-- Конец настроек кнопок напряжения-->
                                    </Grid>
                                    <Grid Grid.Row="9" Grid.Column="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <GroupBox Grid.Row="0" Grid.Column="0" Grid.RowSpan="2">
                                            <GroupBox.Header>
                                                <CheckBox>Питание БП</CheckBox>
                                            </GroupBox.Header>

                                        </GroupBox>
                                        <RadioButton Margin="10, 15, 0, 0">Отключать питание БП в одной секции</RadioButton>
                                        <RadioButton Grid.Row="1" Margin="10, 0, 0, 0">Отключать питание БП во всех секциях</RadioButton>
                                    </Grid>

                                </Grid>
                            </Grid>


                        </TabItem>
                        <TabItem x:Name="reports">
                            <TabItem.Header>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Margin="3" Width="100" TextWrapping="Wrap">Отчеты и критерии норма/отказ</TextBlock>
                                </StackPanel>
                            </TabItem.Header>
                        </TabItem>
                    </TabControl>
                </TabItem.Content>
            </TabItem>
            <TabItem x:Name="settingsOfCells">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Rectangle Height="10" Width="10" Fill="Black" />
                        <TextBlock Margin="3">Настройки секции и ячеек</TextBlock>
                    </StackPanel>
                </TabItem.Header>
                <TabItem.Content>
                    <TabControl x:Name="sections" Margin="10,0,10,150">
                        <TabItem x:Name="section1">
                            <TabItem.Header>
                                <StackPanel Orientation="Horizontal">
                                    <Ellipse Height="10" Width="10" Fill="Black" />
                                    <TextBlock Margin="3">Секция 1</TextBlock>
                                </StackPanel>
                            </TabItem.Header>
                            <TabItem.Content>
                                <StackPanel Margin="10">
                                    <UniformGrid Rows="2" Columns="4" Margin="0, 0, 0, 20">
                                        <CheckBox x:Name="cell1" IsThreeState="True" IsChecked="False" Height="20" Content="Ячейка 1" Width="100"/>
                                        <CheckBox x:Name="cell2" IsThreeState="True" IsChecked="False" Height="20" Content="Ячейка 2" Width="100"/>
                                        <CheckBox x:Name="cell3" IsThreeState="True" IsChecked="False" Height="20" Content="Ячейка 3" Width="100"/>
                                        <CheckBox x:Name="cell4" IsThreeState="True" IsChecked="False" Height="20" Content="Ячейка 4" Width="100"/>
                                        <CheckBox x:Name="cell5" IsThreeState="True" IsChecked="False" Height="20" Content="Ячейка 5" Width="100"/>
                                        <CheckBox x:Name="cell6" IsThreeState="True" IsChecked="False" Height="20" Content="Ячейка 6" Width="100"/>
                                        <CheckBox x:Name="cell7" IsThreeState="True" IsChecked="False" Height="20" Content="Ячейка 7" Width="100"/>
                                        <CheckBox x:Name="cell8" IsThreeState="True" IsChecked="False" Height="20" Content="Ячейка 8" Width="100"/>
                                    </UniformGrid>

                                    <GroupBox Header="Счётчики реле" Padding="5" Height="128">
                                        <Grid>
                                            <DataGrid x:Name="RelayDataGrid" AutoGenerateColumns="False">
                                                <DataGrid.Columns>
                                                    <DataGridTextColumn Header="Ячейка" Binding="{Binding RelayName}" Width="*"/>
                                                    <DataGridTextColumn Header="№1" Binding="{Binding Col1}" Width="*"/>
                                                    <DataGridTextColumn Header="№2" Binding="{Binding Col2}" Width="*"/>
                                                    <DataGridTextColumn Header="№3" Binding="{Binding Col3}" Width="*"/>
                                                    <DataGridTextColumn Header="№4" Binding="{Binding Col4}" Width="*"/>
                                                    <DataGridTextColumn Header="№5" Binding="{Binding Col5}" Width="*"/>
                                                    <DataGridTextColumn Header="№6" Binding="{Binding Col6}" Width="*"/>
                                                    <DataGridTextColumn Header="№7" Binding="{Binding Col7}" Width="*"/>
                                                    <DataGridTextColumn Header="№8" Binding="{Binding Col8}" Width="*"/>
                                                </DataGrid.Columns>
                                            </DataGrid>
                                        </Grid>
                                    </GroupBox>
                                </StackPanel>
                            </TabItem.Content>
                        </TabItem>
                    </TabControl>
                </TabItem.Content>
            </TabItem>
            <TabItem x:Name="personalSettings">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Ellipse Height="10" Width="15" Fill="Black" />
                        <TextBlock Margin="3">Настройки секции и ячеек</TextBlock>
                    </StackPanel>
                </TabItem.Header>
                <TabItem.Content>
                    <TabControl x:Name="personalModes" Margin="10,0,10,150">
                        <StackPanel Height="365">
                            <GroupBox Padding="5">
                                <GroupBox.Header>
                                    <CheckBox  IsChecked="True" Background="Lavender">Режим "Прогрев"</CheckBox>
                                </GroupBox.Header>
                                <StackPanel>
                                    <TextBlock Text="Продолжительность режима:" Margin="0,10,0,0"/>
                                    <TextBox x:Name="ModeDuration" Height="20" Width="60" SelectionBrush="Black" Margin="0 0 650 0"/>
                                </StackPanel>
                            </GroupBox>
                        </StackPanel>
                    </TabControl>
                </TabItem.Content>
            </TabItem>
        </TabControl>

    </StackPanel>

</Page>
